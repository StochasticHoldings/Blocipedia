<div>
  <h1>
  All Wikis
  </h1>
</div>

<div class="row">
  <div class="col-md-8">
    <div>
      <%= link_to "Create a new Wiki", new_wiki_path %>
    </div>

  </div>

<div class="col-md-4">
  <% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
      </h4>
    </div>
  </div>
<% end %>
</div>

  <div class="col-md-4">
    <h3>
      <div>
        <small>
        </small>
      </div>
    </h3>
  </div>

</div>
